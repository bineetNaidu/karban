  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bineetNaidu/karban/issues"
  },
  "homepage": "https://github.com/bineetNaidu/karban#readme",
  "devDependencies": {
    "concurrently": "^6.1.0",
    "lerna": "^4.0.0"
  }
}
